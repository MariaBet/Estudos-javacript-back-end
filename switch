var numero = prompt("Digite um número entre 1 a 3", "Digite aqui");

switch(numero){
  case "1":
    console.log("Numero 1");
    break;
   case "2":
    console.log("Numero 2");
    break;
   case "3":
    console.log("Numero 3");
    break;
  default:
    console.log("Opção inválida");
    break;
}
